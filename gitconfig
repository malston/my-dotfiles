[core]
  editor = /usr/local/bin/code
  autocrlf = input
[transfer]
  fsckobjects = true
[push]
	default = simple
[alias]
  pu = push
  st = status
  di = diff
  co = checkout
  ci = commit
  br = branch
  sta = stash
  llog = log --date=local
  flog = log --pretty=fuller --decorate
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  lol = log --graph --decorate --oneline
  lola = log --graph --decorate --oneline --all
  blog = log origin/master... --left-right
  ds = diff --staged
  fixup = commit --fixup
  squash = commit --squash
  unstage = reset HEAD
  rum = rebase master@{u}
  sur="submodule update --init --recursive"
  cr = clone --recursive
[user]
	name = Amber Alston
	email = amber@replicated.com
[credential]
	credentialStore = cache
	helper = osxkeychain
[init]
	defaultBranch = main
